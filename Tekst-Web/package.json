{
  "name": "tekst",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "prestart": "node scripts/parseHelpTexts.mjs; eslint src/assets/help/index.ts --fix",
    "build-only": "vite build",
    "build": "run-p type-check build-only",
    "prebuild": "node scripts/parseHelpTexts.mjs; eslint src/assets/help/index.ts --fix",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier . --write",
    "prepare": "node scripts/parseHelpTexts.mjs; eslint src/assets/help/index.ts --fix"
  },
  "dependencies": {
    "@tiptap/extension-character-count": "^2.1.12",
    "@tiptap/extension-image": "^2.1.12",
    "@tiptap/extension-link": "^2.1.12",
    "@tiptap/extension-text-align": "^2.1.12",
    "@tiptap/pm": "^2.1.12",
    "@tiptap/starter-kit": "^2.1.12",
    "@tiptap/vue-3": "^2.1.12",
    "@vueuse/core": "^10.3.0",
    "color": "^4.2.3",
    "js-cookie": "^3.0.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "normalize.css": "^8.0.1",
    "openapi-fetch": "^0.8.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/color": "^3.0.3",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/node": "^18.17.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.16.1",
    "isomorphic-dompurify": "^1.9.0",
    "marked": "^9.1.2",
    "naive-ui": "^2.34.4",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^6.5.3",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-checker": "^0.6.1",
    "vue-tsc": "^1.8.6"
