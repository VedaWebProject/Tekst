{
  "name": "tekst",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "api-schema": "node scripts/schemaTransform.mjs; eslint ./src/api/schema.d.ts --fix",
    "help-texts": "node scripts/parseHelpTexts.mjs; eslint src/assets/help/index.ts --fix",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "run-p help-texts api-schema",
    "prebuild": "run-s prepare",
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "start": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tiptap/extension-character-count": "^2.1.12",
    "@tiptap/extension-image": "^2.1.12",
    "@tiptap/extension-link": "^2.1.12",
    "@tiptap/extension-text-align": "^2.1.12",
    "@tiptap/pm": "^2.1.12",
    "@tiptap/starter-kit": "^2.1.12",
    "@tiptap/vue-3": "^2.1.12",
    "@vueuse/core": "^10.3.0",
    "color": "^4.2.3",
    "js-cookie": "^3.0.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "normalize.css": "^8.0.1",
    "openapi-fetch": "^0.8.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-i18n": "^9.3.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/color": "^3.0.3",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/node": "^20.11.10",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "isomorphic-dompurify": "^2.4.0",
    "marked": "^12.0.1",
    "naive-ui": "^2.34.4",
    "npm-run-all2": "^6.1.2",
    "openapi-typescript": "^6.5.3",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.11.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
