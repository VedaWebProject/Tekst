{
  "name": "tekst",
  "version": "0.32.2-alpha.0",
  "private": true,
  "type": "module",
  "scripts": {
    "api-schema": "node prepare/schemaTransform.mjs; eslint src/api/schema.d.ts --fix; prettier --write src/api/schema.d.ts",
    "translations": "node prepare/parseUiTranslations.mjs; node prepare/parseHelpTranslations.mjs; eslint src/assets/i18n/help/index.ts --fix",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ prepare/",
    "prepare": "run-p api-schema translations",
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "start": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-json": "^6.0.1",
    "@tiptap/extension-character-count": "^3.0.0",
    "@tiptap/extension-image": "^3.0.0",
    "@tiptap/extension-table": "^3.0.0",
    "@tiptap/extension-text-align": "^3.0.0",
    "@tiptap/pm": "^3.0.0",
    "@tiptap/starter-kit": "^3.0.0",
    "@tiptap/vue-3": "^3.0.0",
    "@vueuse/core": "^13.0.0",
    "codemirror": "^6.0.1",
    "color2k": "^2.0.3",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "known-css-properties": "^0.37.0",
    "normalize.css": "^8.0.1",
    "openapi-fetch": "^0.14.0",
    "pinia": "^3.0.0",
    "thememirror": "^2.0.1",
    "vue": "^3.5.6",
    "vue-codemirror": "^6.1.1",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@intlify/eslint-plugin-vue-i18n": "^4.0.0",
    "@intlify/unplugin-vue-i18n": "^11.0.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/color": "^4.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.0.0",
    "@vicons/material": "^0.13.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/tsconfig": "^0.8.0",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^10.0.0",
    "marked": "^16.0.0",
    "naive-ui": "^2.42.0",
    "npm-run-all2": "^8.0.0",
    "openapi-typescript": "^7.4.1",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vue-tsc": "^3.0.0",
    "yaml": "^2.7.0"
  }
}
